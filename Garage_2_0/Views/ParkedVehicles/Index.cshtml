@*@model IEnumerable<Garage_2_0.Models.ParkedVehicle>*@
@model Garage_2_0.ViewModels.OverviewModel

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("CheckIn", "CheckIn")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Vehicles.First().Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vehicles.First().RegNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vehicles.First().Color)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vehicles.First().StartTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model.Vehicles) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RegNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Color)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartTime)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("CheckOut", "CheckOut", new { id=item.Id })
        </td>
    </tr>
}

</table>
